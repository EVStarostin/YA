{
    "extends": "airbnb-base",
    "rules": {
        "import/prefer-default-export": 0,
        "no-use-before-define": 0,
        "no-plusplus": 0,
        "prefer-destructuring": 0,
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-console": 0,
        "import/no-unresolved": 0,
        "no-bitwise": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
}